{
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "jest": true
    },
    "plugins": [
        "react",
        "react-native",
        "import",
        "jsx-a11y"
    ],
    "parser": "babel-eslint",
    "extends": [
        "airbnb-base",
        "plugin:react/recommended",
        "plugin:jsx-a11y/recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "no-use-before-define": 0,
        "import/prefer-default-export": 0,
        "react/display-name": 0
    }
}